    <!--Credit Card-->
    <form method="post">
    <section id="oof" style="margin-top:750px">

        <div id="FullName">
            <label class="label" for="message" style="width: 15%;">Full Name :</label>
            <input asp-for="Name" id="message" type="text" placeholder="Full Name" class="firstname" required>
        </div>

        <div id="NameOnCard">
            <label class="label" for="message" style="width: 17%;">Name on Card :</label>
            <input asp-for="NameOnCard" id="message" type="text" placeholder="Name on Card" required>
        </div>

        <div id="CardNumberProfile">
            <label class="label" for="message" style="width: 23%;">Card Number :</label>
            <input asp-for="CardNumber" id="card" placeholder="Card Number" data-mask="0000-0000-0000-0000" type="text" class="card-number-input"  required>
        </div>


        <div class="flex">
            <div id="ExpireDate" style="width: 33%;">
                <label class="label" for="message" style="width: 25%;">Card Date :</label>
                <input asp-for="ExpiredDate" id="message" type="text" data-mask="00/00" placeholder="00/00" required>
            </div>

            <div id="CVV" style="width: 33%;">
                <label class="label" for="message" style="width: 15%;">CVV :</label>
                <input asp-for="Cvv" id="card" type="text" data-mask="000" placeholder="123" required>
            </div>

            <div id="Total" style="width: 33%;">
                <label class="label" for="message" style="width: 15%;">Total :</label>
                <input asp-for="Price" id="card" type="text" class="total"  readonly>
            </div>

                
               

            <div class="center" style="padding-top: 80px;position: absolute;margin-left: 240px;width: 30%;">
                <input asp-route-id="@Model.Flights.Id" type="submit" value="Pay Ticket" class="submit-btn">
            </div>
        </div>
        
                <input asp-for="Seat" id="card" type="text" class="seatnumber" style="visibility:hidden;" readonly>

                <input asp-for="TypeSeat" id="card" type="text" class="typeseat" style="visibility:hidden;" readonly>
    </section>
    </form>
