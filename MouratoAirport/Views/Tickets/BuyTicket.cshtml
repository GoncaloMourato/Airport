    <script>
        $("span").click(function() {
            if ($(this).css("background-color") == "rgb(151, 94, 204)") {
            var valor = "Deluxe";
            var preco = @Model.Flights.Deluxe;
            document.querySelector('.classe').innerText = valor;
            document.querySelector('.price').innerText = preco;
            document.querySelector('.typeseat').value = valor;
            document.querySelector('.total').value = preco;
            
            }
            if ($(this).css("background-color") == "rgb(204, 197, 94)") {
            var valor = "Business";
            var preco = @Model.Flights.Business;
            document.querySelector('.classe').innerText = valor;
            document.querySelector('.price').innerText = preco;
            document.querySelector('.typeseat').value = valor;
            document.querySelector('.total').value = preco;
            }
            if ($(this).css("background-color") == "rgb(94, 109, 204)") {
                var valor = "Economic";
                var preco = @Model.Flights.Economic;
                document.querySelector('.classe').innerText = valor;
                document.querySelector('.price').innerText = preco;
                document.querySelector('.typeseat').value = valor;
                document.querySelector('.total').value = preco;
            }
            var valorSpan = this.getAttribute("value");
            document.querySelector('.minhaLabel').innerText = valorSpan;
            document.querySelector('.seatnumber').value = valorSpan;
        });
    </script>

